<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:t="sap.ui.table">
	<t:TreeTable id="idTableProject" rows="{ path:'CV>/Projects', parameters:{ arrayNames:['Detail'] } }" ariaLabelledBy="title"
		enableSelectAll="false" selectionMode="None" visibleRowCount="5" toggleOpenState="TOS">
		<t:columns>
			<t:Column hAlign="Center" label="Type">
				<t:template>
					<Text text="{CV>Type}"/>
				</t:template>
			</t:Column>
			<t:Column hAlign="Center" label="Name">
				<t:template>
					<Text text="{CV>Name}"/>
				</t:template>
			</t:Column>
			<t:Column hAlign="Center" label="Firm">
				<t:template>
					<Text text="{CV>Firm}"/>
				</t:template>
			</t:Column>
			<t:Column hAlign="Center" label="Role">
				<t:template>
					<Text text="{CV>Role}"/>
				</t:template>
			</t:Column>
			<t:Column hAlign="Center" label="Duration">
				<t:template>
					<Text text="{CV>Date}"/>
				</t:template>
			</t:Column>
		</t:columns>
	</t:TreeTable>
</core:FragmentDefinition>