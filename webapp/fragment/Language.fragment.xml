<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Table items="{CV>/Languages}">
		<columns>
			<Column hAlign="Center">
				<Label text="Language" design="Bold"/>
			</Column>
			<Column hAlign="Center">
				<Label text="Degree" design="Bold"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{CV>Language}"/>
					<RatingIndicator maxValue="5" editable="false" value="{CV>Degree}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>